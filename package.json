{
  "name": "telegram-search",
  "type": "module",
  "version": "1.0.0-beta.22",
  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "dev:server": "pnpm -F @tg-search/server dev",
    "dev:frontend": "pnpm -F @tg-search/frontend dev",
    "dev:electron": "pnpm -F @tg-search/electron dev",
    "build:packages": "pnpm -rF=\"./packages/*\" --parallel build",
    "packages:build": "pnpm -rF=\"./packages/*\" --parallel build",
    "test": "vitest",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache . --fix",
    "typecheck": "pnpm -r --parallel typecheck",
    "bump": "bumpp",
    "bump:deps": "taze -I -f -w -r",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx scripts/db.ts migrate",
    "db:push": "tsx scripts/db.ts push",
    "db:kit": "tsx scripts/db.ts",
    "postinstall": "pnpm run packages:build"
  },
  "devDependencies": {
    "@proj-airi/unocss-preset-chromatic": "^1.0.0",
    "@types/node": "^24.2.0",
    "@unbird/eslint-config": "^1.0.1",
    "@unbird/logg": "^1.1.2",
    "bumpp": "^10.2.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.33.0",
    "tailwind-merge": "^3.3.1",
    "taze": "^19.1.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "unocss": "^66.4.2",
    "unocss-preset-animations": "^1.2.1",
    "unocss-preset-shadcn": "^0.5.0",
    "unplugin-unused": "^0.5.1",
    "vite": "npm:rolldown-vite@latest",
    "vitest": "^3.2.4"
  }
}
